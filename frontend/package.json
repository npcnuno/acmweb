{
  "name": "adminpanelsveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm run wasm && vite dev --host 0.0.0.0 --port 3000",
    "build": "npm run wasm && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "modulize-wasm": "node ./wasm-test/modulize.js",
    "wasm": "wasm-pack build ./wasm-test --target web && npm run modulize-wasm"
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "^2.8.0",
    "@skeletonlabs/tw-plugin": "^0.3.1",
    "@sveltejs/adapter-node": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "sveltekit-superforms": "^2.1.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "typescript-svelte-plugin": "^0.3.37",
    "valibot": "^0.37.0",
    "vite": "^5.0.12",
    "vite-plugin-tailwind-purgecss": "^0.2.0"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.6.13",
