    "fs": "^0.0.1-security",
    "lucide-svelte": "^0.476.0",
    "mode-watcher": "^0.5.1",
    "simplex-noise": "^4.0.3",
    "superforms": "^0.0.1",
    "svelte-chartjs": "^3.1.5",
    "svelte-i18n": "^4.0.1",
    "svelte-inview": "^4.0.4",
    "svelte-markdown": "^0.4.1",
    "svelte-motion": "^0.12.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^3.4.1",
    "vite-plugin-wasm-pack": "^0.1.12",
    "yup": "^1.3.3"
